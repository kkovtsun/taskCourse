<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation=
                 "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>Students personnel</display-name>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <description></description>
        <servlet-name>WelcomeUser</servlet-name>
        <servlet-class>WelcomeUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WelcomeUser</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegistrationUser</servlet-name>
        <servlet-class>RegistrationUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationUser</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MainUserFrame</servlet-name>
        <servlet-class>MainUserFrame</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainUserFrame</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MainModeratorFrame</servlet-name>
        <servlet-class>MainModeratorFrame</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainModeratorFrame</servlet-name>
        <url-pattern>/mainmoderator</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>MainAdminFrame</servlet-name>
        <servlet-class>MainAdminFrame</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainAdminFrame</servlet-name>
        <url-pattern>/mainadmin</url-pattern>
    </servlet-mapping>

</web-app>